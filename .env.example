# snapMULTI Environment Configuration
# Copy this file to .env and update with your values

# Music library path (host directory to mount)
# Mount your music here before starting: /media/music
# Example: sudo mount /dev/sda1 /media/music
MUSIC_PATH=/media/music

# Timezone
TZ=Europe/Berlin

# User/Group for container processes
# These should match your host user to avoid permission issues
PUID=1000
PGID=1000
